.section{ position: absolute; width: 100%; height: 100% ; overflow: hidden;}
.types{ position: absolute; left: 0; height: 100%; width: 90px; overflow: auto;background: #F6F6F6; text-align: center;font-size:13px;}
.types:after{ content: ""; position: absolute; background: #dcdcdc; right: 0; top: 0; height: 100%; border-right: 1px solid #dcdcdc;}
.types .active{ background: #FFF; position: relative; z-index: 2;}
.types view{ padding:20px 0;border-bottom: 1px solid #dcdcdc;}

.typeTree{ padding: 10px 20px;margin-left: 90px; width:auto;height: 100%; overflow: auto; background: #fff;font-size: 12px}
.typeTree .list{ display: flex; flex-wrap: wrap; overflow: hidden;padding-bottom: 10px;}
.typeTree .item{text-align: center; width: 33.333%}
.typeTree .title{ padding: 10px 0; color: #333;}
.typeTree .item image{ width: 75px;height: 75px;}
.typeTree .item .name{ padding-bottom: 10px; display: block;word-break:break-all;color: #666; }

